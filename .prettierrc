{
	"semi": true,
	"printWidth": 90,
	"endOfLine": "lf",
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"plugins": ["./node_modules/prettier-plugin-tailwindcss"],
	"quoteProps": "as-needed",
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true
}
