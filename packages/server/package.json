		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@nabla/vite-plugin-eslint": "^1.4.0",
		"@swc/core": "^1.2.197",
		"@types/express": "^4.17.13",
		"@types/express-sslify": "^1.2.2",
		"@types/jest": "^27.5.2",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.41",
		"@types/supertest": "^2.0.12",
		"@types/throng": "^5.0.3",
		"esbuild": "^0.14.43",
		"esbuild-jest": "^0.5.0",
		"jest": "^28.1.1",
		"jsdom": "^19.0.0",
		"supertest": "^6.2.3",
