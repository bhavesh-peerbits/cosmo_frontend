{
	"compilerOptions": {
		"lib": ["es2019", "es2020.promise", "es2020.bigint", "es2020.string"],
		"module": "CommonJS",
		"target": "ES2019",
		"declaration": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"inlineSources": true,
		"skipLibCheck": true,
		"baseUrl": ".",
		"jsx": "react-jsx"
	},
	"include": [".storybook/**.ts", "stories/**/*.tsx", "./defs.d.ts"]
}
