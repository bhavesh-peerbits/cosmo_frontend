{
	"name": "app",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint:fix": "eslint --cache --fix --ignore-path ../.gitignore --ext .ts,.tsx .",
		"lint:stylelint": "stylelint --cache --cache-location node_modules/.cache/ --fix --ignore-path .gitignore '**/*.[s]css'",
		"lint": "tsc && yarn lint:fix && yarn lint:stylelint",
		"openapi:generate": "cross-env PWD= JAVA_OPTS=\"-Dio.swagger.parser.util.RemoteUrl.trustAll=true -Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true\" openapi-generator-cli generate",
		"test:unit": "vitest",
		"test:unit:ci": "vitest run",
		"test:e2e": "cypress open --config-file cypress.dev.json",
		"test:e2e:ci": "start-server-and-test preview http://localhost:4173 'cypress run'",
		"test:ci": "yarn test:unit:ci && yarn test:e2e:ci",
		"validate": "yarn lint && yarn test:unit:ci && yarn test:e2e:ci",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook -o dist/storybook"
	},
	"dependencies": {
		"@carbon/react": "^0.17.0",
		"@exceptionless/react": "^2.0.0",
		"@sentry/react": "6.18.2",
		"@sentry/tracing": "6.18.2",
		"axios": "^0.26.1",
		"i18next": "^21.6.14",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-error-boundary": "^3.1.4",
		"react-hook-form": "^7.29.0",
		"react-i18next": "^11.16.2",
		"react-query": "^3.34.19",
		"react-router-dom": "^6.2.2",
		"recoil": "^0.6.1",
		"translation-check": "^1.0.2",
		"workbox-recipes": "^6.5.1",
		"workbox-routing": "^6.5.1",
		"workbox-strategies": "^6.5.1"
	},
	"devDependencies": {
		"@babel/core": "^7.17.8",
		"@cypress/code-coverage": "^3.9.12",
		"@openapitools/openapi-generator-cli": "^2.4.26",
		"@storybook/addon-actions": "^6.4.19",
		"@storybook/addon-essentials": "^6.4.19",
		"@storybook/addon-interactions": "^6.4.19",
		"@storybook/addon-links": "^6.4.19",
		"@storybook/react": "^6.4.19",
		"@storybook/testing-library": "^0.0.9",
		"@testing-library/cypress": "^8.0.2",
		"@testing-library/dom": "^8.11.3",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^13.0.0-alpha.6",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/css-mediaquery": "^0.1.1",
		"@vitejs/plugin-legacy": "^1.7.1",
		"@vitejs/plugin-react": "^1.2.0",
		"autoprefixer": "^10.4.4",
		"babel-loader": "^8.2.3",
		"c8": "^7.11.0",
		"cross-env": "^7.0.3",
		"css-mediaquery": "^0.1.2",
		"cypress": "^9.5.2",
		"jsdom": "^19.0.0",
		"msw": "^0.39.2",
		"postcss": "^8.4.12",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"sass": "^1.49.9",
		"start-server-and-test": "^1.14.0",
		"storybook-builder-vite": "^0.1.21",
		"stylelint": "^14.6.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"tailwindcss": "^3.0.23",
		"vite-plugin-pwa": "^0.11.13",
		"vitest": "^0.8.1",
		"whatwg-fetch": "^3.6.2",
		"workbox-build": "^6.5.1",
		"workbox-window": "^6.5.1",
		"yarn-deduplicate": "^3.1.0"
	}
}
