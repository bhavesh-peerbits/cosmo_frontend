{
	"name": "server",
	"version": "1.0.0",
	"description": "Cosmo Backend server",
	"private": true,
	"scripts": {
		"start": "yarn node index.js",
		"dev": "vite",
		"build": "yarn tsc && vite build",
		"lint:fix": "yarn eslint --cache --fix --ignore-path ../.gitignore --ext .ts .",
		"lint": "yarn tsc && yarn lint:fix",
		"test:ci": "jest",
		"validate": "yarn lint && yarn test",
		"tsc": "yarn run -T tsc",
		"eslint": "yarn run -T eslint"
	},
	"dependencies": {
		"esbuild-jest": "^0.5.0",
		"express": "5.0.0-beta.1",
		"express-sslify": "^1.2.0",
		"helmet": "^5.0.2",
		"morgan": "^1.10.0",
		"throng": "^5.0.0",
		"winston": "^3.6.0"
	},
	"devDependencies": {
		"@nabla/vite-plugin-eslint": "^1.4.0",
		"@swc/core": "^1.2.162",
		"@types/express": "^4.17.13",
		"@types/express-sslify": "^1.2.2",
		"@types/jest": "^27.4.1",
		"@types/morgan": "^1.9.3",
		"@types/node": "^17.0.23",
		"@types/supertest": "^2.0.12",
		"@types/throng": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^5.15.0",
		"@typescript-eslint/parser": "^5.15.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^16.1.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.5.1",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"vite": "^2.9.1",
		"vite-plugin-node": "^0.0.19",
		"vite-tsconfig-paths": "^3.4.1"
	},
	"peerDependencies": {
		"esbuild": "*",
		"eslint": "*",
		"eslint-plugin-jsx-a11y": "*",
		"eslint-plugin-react": "*",
		"eslint-plugin-react-hooks": "*",
		"prettier": "*",
		"typescript": "*"
	}
}
